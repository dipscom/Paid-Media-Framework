package {	import com.pedrotavares.paidmedia.platforms.doubleclick.DoubleclickVideo;		import flash.events.Event;	public class VideoMain extends DoubleclickVideo	{		public function VideoMain()		{			trace("[RICH] All good");			// Uncomment the bellow later			addEventListener(Event.ENTER_FRAME, getDate);		}				public function getDate(e:Event):void		{			if(dateVar)			{				removeEventListener(Event.ENTER_FRAME, getDate);				setupVideo("_SampleVideo.flv", video_holder, play_toggle, sound_toggle, replay_mc, 300,128);								playVideo();			}			trace( dateVar );		}	}}