package {	import com.pedrotavares.paidmedia.platforms.doubleclick.*;		import flash.events.Event;	public class VidMain extends RichVideo	{		public function VidMain()		{			trace("[DOUBLECLICK RICH] All good");						// Listen for the added to stage			this.addEventListener( Event.ADDED_TO_STAGE, onStage );		}				private function onStage(e:Event):void		{			trace("[DOUBLECLICK RICH] On stage");			// Initialize the base classes			setupAd();			// Initialize the video			setupVideo("_SampleVideo.flv", video_holder, play_toggle, sound_toggle, replay_mc, 300,128);			// Automatically play the video			playVideo();		}	}}