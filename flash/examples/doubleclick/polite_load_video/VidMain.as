package {	import com.pedrotavares.paidmedia.platforms.doubleclick.DoubleclickVideo;		import flash.events.Event;	public class VidMain extends DoubleclickVideo	{		public function VidMain()		{			trace("[RICH] All good");			// Initialize the base classes			startDcVideo();			setupVideo("_SampleVideo.flv", video_holder, play_toggle, sound_toggle, replay_mc, 300,128);			// Listen for the added to stage			this.addEventListener( Event.ADDED_TO_STAGE, onStage );		}				private function onStage(e:Event):void		{			trace("[RICH] On stage");			playVideo();		}	}}