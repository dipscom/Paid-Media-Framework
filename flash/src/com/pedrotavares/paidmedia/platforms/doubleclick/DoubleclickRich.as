package  com.pedrotavares.paidmedia.platforms.doubleclick{	// CUSTOM CLASSE IMPORTS	import com.pedrotavares.paidmedia.platforms.base.BaseRich;	import com.pedrotavares.paidmedia.events.PaidMediaEvent;	// DOUBLECLICK IMPORTS	import com.google.ads.studio.ProxyEnabler;		public class DoubleclickRich extends BaseRich	{		// Get an instance of the enabler		public var enabler:ProxyEnabler = ProxyEnabler.getInstance();					public function DoubleclickRich() 		{			// Listen for the click			addEventListener(PaidMediaEvent.CLICK_CLICKTAG, dcRichClick);		}				protected function startDcRich():void		{			// Start the base class			setupAd();		}				protected function dcRichClick(e:PaidMediaEvent):void		{			enabler.exit("Main Click");		}	}}