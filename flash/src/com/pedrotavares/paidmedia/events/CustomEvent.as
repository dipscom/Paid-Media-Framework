package com.pedrotavares.paidmedia.events{	import flash.events.Event;	public class CustomEvent extends Event	{		public static const EVNT:String = "CustomEvent_Event";				public var params:Object;				public function CustomEvent( tpe:String, prms:Object )		{			// Brinng in all the Event attributes			super( tpe, true, true );			// Make the parameters a global variable			this.params = prms;		}				/*		// Override the flash.events.Event clone method		// return Event		*/		public override function clone():Event		{			return new CustomEvent( this.type, this.params );		}				/*		// Override the flash.events.Event toString method		// return String - Class name		*/		override public function toString():String		{			return( "[Event CustomEvent]" );		}	}}