package com.pedrotavares.paidmedia.events{	import flash.events.Event;		public class PaidMediaEvent extends Event	{		// MOUSE EVENTS		public static const MOUSE_CLICK				:String = "PaidMediaEvent_MouseClick";		public static const MOUSE_OVER				:String = "PaidMediaEvent_MouseOver";		public static const MOUSE_OUT				:String = "PaidMediaEvent_MouseOut";		public static const CLICK_CLICKTAG			:String = "PaidMediaEvent_ClickClickTag";				// VIDEO EVENTS		public static const VIDEO_INITIALISED		:String	= "PaidMediaEvent_VideoInitialised";		public static const ON_VIDEO_CUE			:String = "PaidMediaEvent_OnVideoCue";		public static const VIDEO_PLAY				:String = "PaidMediaEvent_VideoPlay";		public static const VIDEO_REPLAY			:String = "PaidMediaEvent_VideoReplay";		public static const ON_VIDEO_END			:String = "PaidMediaEvent_OnVideoEnd";				// EXPANSION / COLLAPSE EVENTS		public static const UNIT_EXPAND				:String = "PaidMediaEvent_Expand";		public static const UNIT_COLLAPSE			:String = "PaidMediaEvent_Collapse";				// OTHER EVENTS		public static const LOAD_COMPLETE			:String = "PaidMediaEvent_LoadComplete";		public static const UNIT_INITIALISED		:String = "PaidMediaEvent_UnitInitialised";		public static const COMPLETED				:String = "PaidMediaEvent_Completed";				public function PaidMediaEvent( eventType :String, bubbles :Boolean = true, cancelable :Boolean = false )		{			super( eventType, bubbles, cancelable );		}				override public function clone():Event		{			// Return a new instance of this event with the same parameters.			return new PaidMediaEvent(type, bubbles, cancelable);		}	}}